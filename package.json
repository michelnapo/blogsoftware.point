{
  "name": "template.point",
  "version": "0.0.1",
  "author": "Point Network",
  "description": "Template example zapp - use as a scaffold to create more Zapps!",
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@openzeppelin/contracts": "^4.4.2",
    "@openzeppelin/contracts-upgradeable": "^4.6.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-quill": "^1.3.5",
    "react-scripts": "4.0.3",
    "timeago.js": "^4.0.2",
    "web-vitals": "^1.0.1",
    "wouter": "^2.7.4"
  },
  "scripts": {
    "clear": "rm -rf ./public/* .cache .parcel-cache",
    "start": "react-scripts start",
    "watch:docker": "./watch.docker.js",
    "build": "npm run clear && cp src/*.html ./public/. && cp src/*.css ./public/. && parcel build src/index.js --dist-dir ./public --no-source-maps",
    "watch": "npm run clear && cp src/*.html ./public/. && cp src/*.css ./public/. && parcel watch src/index.js --dist-dir ./public/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@parcel/transformer-image": "^2.0.0-rc.0",
    "autoprefixer": "^10.4.7",
    "parcel": "^2.0.0-rc.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.6"
  }
}
